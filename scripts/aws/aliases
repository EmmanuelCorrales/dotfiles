# Internet Gateway
alias ig-list-ids='aws ec2 describe-internet-gateways \
  | jq -r .InternetGateways[].InternetGatewayId'

# VPC
alias vpc-list-ids='aws ec2 describe-vpcs | jq -r .Vpcs[].VpcId'

alias vpc-list-default-ids='aws ec2 describe-vpcs \
  --filters Name=isDefault,Values=true | jq -r .Vpcs[].VpcId'

alias vpc-list-custom-ids='aws ec2 describe-vpcs \
  --filters Name=isDefault,Values=false | jq -r .Vpcs[].VpcId'

# Subnet
alias subnet-list-ids='aws ec2 describe-subnets \
  | jq -r .Subnets[].SubnetId'

# EC2
alias ec2-list-reserved-ids='aws ec2 describe-instances \
  | jq -r .Reservations[].Instances[].InstanceId'